{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": ["error"],
        "import/extensions": ["error", "always"]
    }
}
